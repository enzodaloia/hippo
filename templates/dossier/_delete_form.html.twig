<form method="post" action="{{ path('app_dossier_delete', {'token': dossier.token}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ dossier.token) }}">
    <button class="btn btn-danger"><i class="fa fa-trash"></i></button>
</form>
